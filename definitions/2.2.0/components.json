{
  "type": "object",
  "description": "An object to hold a set of reusable objects for different aspects of the AsyncAPI Specification.",
  "additionalProperties": false,
  "patternProperties": {
    "^x-[\\w\\d.\\-_]+$": {
      "$ref": "http://asyncapi.com/draft-07/schemas/specificationExtension"
    }
  },
  "properties": {
    "schemas": {
      "$ref": "http://asyncapi.com/draft-07/schemas/schemas"
    },
    "messages": {
      "$ref": "http://asyncapi.com/draft-07/schemas/messages"
    },
    "securitySchemes": {
      "type": "object",
      "patternProperties": {
        "^[\\w\\d\\.\\-_]+$": {
          "oneOf": [
            {
              "$ref": "http://asyncapi.com/draft-07/schemas/Reference"
            },
            {
              "$ref": "http://asyncapi.com/draft-07/schemas/SecurityScheme"
            }
          ]
        }
      }
    },
    "parameters": {
      "$ref": "http://asyncapi.com/draft-07/schemas/parameters"
    },
    "correlationIds": {
      "type": "object",
      "patternProperties": {
        "^[\\w\\d\\.\\-_]+$": {
          "oneOf": [
            {
              "$ref": "http://asyncapi.com/draft-07/schemas/Reference"
            },
            {
              "$ref": "http://asyncapi.com/draft-07/schemas/correlationId"
            }
          ]
        }
      }
    },
    "operationTraits": {
      "type": "object",
      "additionalProperties": {
        "$ref": "http://asyncapi.com/draft-07/schemas/operationTrait"
      }
    },
    "messageTraits": {
      "type": "object",
      "additionalProperties": {
        "$ref": "http://asyncapi.com/draft-07/schemas/messageTrait"
      }
    },
    "serverBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "http://asyncapi.com/draft-07/schemas/bindingsObject"
      }
    },
    "channelBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "http://asyncapi.com/draft-07/schemas/bindingsObject"
      }
    },
    "operationBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "http://asyncapi.com/draft-07/schemas/bindingsObject"
      }
    },
    "messageBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "http://asyncapi.com/draft-07/schemas/bindingsObject"
      }
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://asyncapi.com/draft-07/schemas/components"
}