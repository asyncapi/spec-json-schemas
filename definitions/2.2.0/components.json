{
  "type": "object",
  "description": "An object to hold a set of reusable objects for different aspects of the AsyncAPI Specification.",
  "additionalProperties": false,
  "patternProperties": {
    "^x-[\\w\\d.\\-_]+$": {
      "$ref": "specificationExtension"
    }
  },
  "properties": {
    "schemas": {
      "$ref": "schemas"
    },
    "messages": {
      "$ref": "messages"
    },
    "securitySchemes": {
      "type": "object",
      "patternProperties": {
        "^[\\w\\d\\.\\-_]+$": {
          "oneOf": [
            {
              "$ref": "Reference"
            },
            {
              "$ref": "SecurityScheme"
            }
          ]
        }
      }
    },
    "parameters": {
      "$ref": "parameters"
    },
    "correlationIds": {
      "type": "object",
      "patternProperties": {
        "^[\\w\\d\\.\\-_]+$": {
          "oneOf": [
            {
              "$ref": "Reference"
            },
            {
              "$ref": "correlationId"
            }
          ]
        }
      }
    },
    "operationTraits": {
      "type": "object",
      "additionalProperties": {
        "$ref": "operationTrait"
      }
    },
    "messageTraits": {
      "type": "object",
      "additionalProperties": {
        "$ref": "messageTrait"
      }
    },
    "serverBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "bindingsObject"
      }
    },
    "channelBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "bindingsObject"
      }
    },
    "operationBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "bindingsObject"
      }
    },
    "messageBindings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "bindingsObject"
      }
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "components"
}