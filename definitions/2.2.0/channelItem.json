{
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^x-[\\w\\d.\\-_]+$": {
      "$ref": "specificationExtension"
    }
  },
  "properties": {
    "$ref": {
      "$ref": "ReferenceObject"
    },
    "parameters": {
      "type": "object",
      "additionalProperties": {
        "$ref": "parameter"
      }
    },
    "description": {
      "type": "string",
      "description": "A description of the channel."
    },
    "servers": {
      "type": "array",
      "description": "The names of the servers on which this channel is available. If absent or empty then this channel must be available on all servers.",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "publish": {
      "$ref": "operation"
    },
    "subscribe": {
      "$ref": "operation"
    },
    "deprecated": {
      "type": "boolean",
      "default": false
    },
    "bindings": {
      "$ref": "bindingsObject"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "channelItem"
}