{
  "type": "object",
  "additionalProperties": false,
  "required": [
    "name"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "externalDocs": {
      "$ref": "./externalDocs.json"
    }
  },
  "patternProperties": {
    "^x-[\\w\\d\\.\\-\\_]+$": {
      "$ref": "./specificationExtension.json"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./tag.json"
}