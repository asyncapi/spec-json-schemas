{
    "type": "object",
    "additionalProperties": false,
    "patternProperties": {
        "^x-[\\w\\d\\.\\-\\_]+$": {
            "$ref": "./specificationExtension.json"
        }
    },
    "properties": {
        "summary": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "tags": {
            "type": "array",
            "items": {
                "$ref": "./tag.json"
            },
            "uniqueItems": true
        },
        "externalDocs": {
            "$ref": "./externalDocs.json"
        },
        "operationId": {
            "type": "string"
        },
        "bindings": {
            "$ref": "./bindingsObject.json"
        }
    },
    "$schema": "http://json-schema.org/draft-07/schema#"
}